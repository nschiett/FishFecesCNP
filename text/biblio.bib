doi = {10.1007/s00227-004-1398-3},
file = {:home/nschiettekatte/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Mills, Sebens - 2004 - Ingestion and assimilation of nitrogen from benthic sediments by three species of coral.pdf:pdf},
