isbn = {0078-3218$\backslash$r0-415-25463-9},
issn = {0078-3218},
journal = {Oceanography and Marine Biology, Vol 41},
keywords = {foraging,grazing,{\{}BIOCHEMICAL-COMPOSITION{\}},{\{}CTENOCHAETUS-STRIATUS{\}},{\{}DAMSELFISHES{\}},{\{}DISSOLVED{\}},{\{}EXOPOLYMER{\}},{\{}FAMILY{\}},{\{}FISHES{\}},{\{}GREAT-BARRIER-REEF{\}},{\{}HERBIVOROUS{\}},{\{}ORGANIC-MATTER{\}},{\{}PARROTFISHES{\}},{\{}PARTICLES{\}},{\{}PATTERNS{\}},{\{}PERIODICITY{\}},{\{}SCARIDAE{\}},{\{}SPATIAL{\}},{\{}TERRITORIAL{\}},{\{}TRANSPARENT{\}}},
